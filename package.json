{
  "name": "etools-epd",
  "description": "Etools ePD",
  "version": "1.0.0",
  "contributors": [
    "eTools Team"
  ],
  "license": "BSD-3-Clause",
  "repository": "https://github.com/unicef/etools-epd",
  "scripts": {
    "tsToJs": "tsc --skipLibCheck",
    "tsToJsOrTrue": "tsc --skipLibCheck || true",
    "tsEslint": "node_modules/eslint/bin/eslint.js 'src_ts/**/*.ts' --fix",
    "start": "tsc --watch --preserveWatchOutput & polymer serve -H 0.0.0.0 -p 8080",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run tsToJsOrTrue && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-label": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-checkbox": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/paper-input": "^3.2.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-progress": "^3.0.1",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-slider": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.2",
    "@polymer/paper-tabs": "^3.0.0",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "@types/leaflet": "^1.7.4",
    "@types/lodash-es": "^4.17.3",
    "@unicef-polymer/etools-ajax": "^4.3.0",
    "@unicef-polymer/etools-behaviors": "^3.0.4",
    "@unicef-polymer/etools-content-panel": "^4.1.0",
    "@unicef-polymer/etools-currency-amount-input": "^3.1.0",
    "@unicef-polymer/etools-data-table": "^5.1.1",
    "@unicef-polymer/etools-date-time": "2.2.0-rc.2",
    "@unicef-polymer/etools-dialog": "5.0.0-rc.2",
    "@unicef-polymer/etools-dropdown": "^3.7.1",
    "@unicef-polymer/etools-info-tooltip": "^3.2.0-rc.3",
    "@unicef-polymer/etools-loading": "5.0.0-rc.2",
    "@unicef-polymer/etools-modules-common": "1.0.0-rc.1",
    "@unicef-polymer/etools-profile-dropdown": "6.0.0-rc.1",
    "@unicef-polymer/etools-table": "^1.2.1",
    "@unicef-polymer/etools-types": "^1.10.0",
    "@unicef-polymer/etools-upload": "^3.5.1",
    "@unicef-polymer/etools-piwik-analytics": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "dayjs": "^1.9.7",
    "focus-visible": "^5.2.0",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.0",
    "lit-element": "^2.3.1",
    "lit-translate": "^1.2.1",
    "lodash-es": "^4.17.15",
    "polymer-cli": "^1.9.11",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/typescript-estree": "^4.33.0",
    "del": "^3.0.0",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "prettier": "~2.4.1",
    "typescript": "^3.9.6",
    "path-parse": ">=1.0.7",
    "hosted-git-info": ">=2.8.9",
    "y18n": ">=3.2.2"
  }
}
